query {
	hero(episode: EMPIRE) {
		id
		name
		... on Droid { primaryFunction }
		... on Human { height(unit: *) mass }
		appearsIn
		friendsConnection {
			totalCount
			friends {
				id name appearsIn
				...on Droid { primaryFunction }
				... on Human { height(unit: *) mass }
			}
		}
	}
	starship(id:"3001") { name length(unit: != METER) }
}
